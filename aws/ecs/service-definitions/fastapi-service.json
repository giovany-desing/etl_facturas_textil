{
  "serviceName": "fastapi-service",
  "cluster": "etl-facturas-cluster",
  "taskDefinition": "fastapi-service",
  "desiredCount": 2,
  "launchType": "FARGATE",
  "networkConfiguration": {
    "awsvpcConfiguration": {
      "subnets": [
        "SUBNET_ID_1",
        "SUBNET_ID_2"
      ],
      "securityGroups": [
        "SG_ID"
      ],
      "assignPublicIp": "ENABLED"
    }
  },
  "loadBalancers": [
    {
      "targetGroupArn": "arn:aws:elasticloadbalancing:us-east-1:ACCOUNT_ID:targetgroup/fastapi-tg/REPLACE_WITH_TARGET_GROUP_ID",
      "containerName": "fastapi",
      "containerPort": 8000
    }
  ],
  "healthCheckGracePeriodSeconds": 60,
  "deploymentConfiguration": {
    "maximumPercent": 200,
    "minimumHealthyPercent": 100,
    "deploymentCircuitBreaker": {
      "enable": true,
      "rollback": true
    }
  },
  "enableExecuteCommand": true,
  "tags": [
    {
      "key": "Environment",
      "value": "production"
    },
    {
      "key": "Service",
      "value": "fastapi"
    },
    {
      "key": "ManagedBy",
      "value": "terraform"
    }
  ]
}

